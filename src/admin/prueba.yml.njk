---
permalink: "admin/prueba.yml"
---


local_backend: true

backend:
  name: git-gateway
  branch: main

media_folder: ""

collections:  
{% for seccion in paginas %}
  - name: "{{ seccion.name }}"
    label: "{{ seccion.title }}"
    folder: "{{ seccion.folder }}"
    create: true
    fields:
      - { label: "Title", name: "title", widget: "string"}
      - { label: "Body", name: "body", widget: "markdown"}  }
{% endfor %}
